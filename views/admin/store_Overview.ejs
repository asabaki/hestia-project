//Rating turn number to stars
   var newSpan='<span class="fa fa-star"></span>' + 
                '<span class="fa fa-star"></span>'+ 
                '<span class="fa fa-star"></span>'+ 
                '<span class="fa fa-star"></span>'+ 
                '<span class="fa fa-star"></span>';

    $("td.starr").append(newSpan);
  
    var n=$("td.starr p").size();

    $("td.starr p").css("display","inline");
    $("td.starr p").css("visibility","hidden");

    for(let j=0;j<n;j++)
    {
        var rate=$("td.starr:eq("+j+")").text();
        for(let i=0;i<rate;i++)
        {
            $( "td.starr:eq("+j+") span.fa.fa-star:eq("+i+")" ).addClass("checked")
        }
    }

