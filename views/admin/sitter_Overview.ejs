      <section id="main-content">
        <section class="wrapper site-min-height">
            <h3><i class="fa fa-angle-right"></i> Blank Page</h3>
           
            <div class="row mt">
              <div class="col-md-12">
                  <div class="content-panel">
                      <table class="table table-striped table-advance table-hover">
                              <h4><i class="fa fa-angle-right"></i> Advanced Table</h4>
                              <hr>
                          <thead>
                          <tr>
                              <th><i class="fas fa-envelope"></i> Email</th>
                              <th><i class="fas fa-user-circle"></i> Name</th>
                              <th><i class="fas fa-map-pin"></i> Sitting Type</th>
                              <th><i class="far fa-calendar-alt"></i> Date started</th>
                              <th><i class="far fa-star"></i> Rate</th>
                              <th><i class="far fa-check-circle"></i> Completed jobs</th>
                              <th><i class="fas fa-hand-holding-usd"></i> Money Earn</th>
                              <th><form><button class="btn btn-danger btn-xs"  formmethod="POST" formaction="/admin/sitter?_method=DELETE"><i class="fas fa-trash-alt"></i></button></form></th>
                              <th></th>
                          </tr>
                          </thead>
                          <tbody>
                              <% sitters.forEach((sitter) => { %>
                          <tr>
                              <td><a href="#"><%=sitter.username%></a></td>
                              <td><%=sitter.firstname+" "+sitter.lastname%></td>
                                <% if(sitter.servicesOffered.houses) { %>
                                    <td>House</td>
                                <% } else if(sitter.servicesOffered.kids) {%>
                                <td>Child</td>
                                <% } else if(sitter.servicesOffered.pets) {%>
                                    <td>Pet</td>
                                <% } else if(sitter.servicesOffered.aged) {%>
                                    <td>Elder</td>
                                <% } else if(sitter.servicesOffered.challenged) {%>
                                    <td>Challenged</td>
                                <% } else {%>
                                    <td>Not Set</td>
                                <% } %>
                              <td><%=sitter.createdAt.toLocaleDateString()%>
                              <td class="starr"><p><%=sitter.rating%></p></td>
                                <td>70</td>
                                <td>30000</td>
                              
                              <td>
                                  <form method="POST">
                                      <% if(!sitter.approval) { %>
                                        <button class="btn btn-success btn-xs" formaction="/admin/sitter/<%=sitter._id%>?_method=PATCH"><i class="fa fa-check"></i></button>
                                        <% } %>
                                        <button class="btn btn-primary btn-xs"><i class="fas fa-pencil-alt"></i></button>
                                        <button class="btn btn-danger btn-xs" formaction="/admin/sitter/<%=sitter._id%>?_method=DELETE"><i class="fas fa-trash-alt"></i></button>
                                  </form>
                                  
                              </td>
                           </tr>
                          <% }) %> 
                          </tbody>
                      </table>
                  </div><!-- /content-panel -->
                  <br>
                  <div class="centered">
                        <div class="btn-group">
                            <% if (_thisPage >= 2) { %>
                            <a href="/admin/sitter/0" class="btn btn-default">&lt;&lt;</a>
                            <% } %>
                            <% if (_thisPage!=1) { %>
                            <a class="btn btn-default" href="/admin/sitter/<%=(_thisPage-2)%>">&lt;</a>
                            <% } %>
                            <% if (_thisPage > 2) { %>
                            <a type="button" class="btn btn-default">...</a>
                            <% } %>
                            <% if (_thisPage!=1) { %>
                            <a type="button" class="btn btn-default" href="/admin/sitter/<%=(_thisPage-2)%>"><%=(_thisPage-1)%></a>
                            <% } %>
                            <a href="#" type="button" class="btn btn-default active"><%=_thisPage%></a>
                            <% if(_thisPage!=page&&page!=0) { %>
                            <a type="button" href="/admin/sitter/<%=_thisPage%>" class="btn btn-default"><%=(1+_thisPage)%></a>
                            <% } %>
                            <% if(_thisPage<page-1) { %>
                            <a type="button" class="btn btn-default">...</a>
                            <% } %>
                            <% if (_thisPage!=page&&page!=0) { %>
                            <a type="button" href="/admin/sitter/<%=_thisPage%>" class="btn btn-default">&gt;</a>
                            <% } %>
                            <% if (_thisPage<page-1) { %>
                            <a type="button" href="/admin/sitter/<%=(page-1)%>" class="btn btn-default">&gt;&gt;</a>
                            <%}%>

                        </div>  
